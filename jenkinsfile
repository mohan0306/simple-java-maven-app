
currentBuild.displayName = "Game-of-Life-#"+currentBuild.number
pipeline{
    agent any
    
    stages {
        stage('continuous download') {
            steps {
                git 'https://github.com/mohan0306/simple-java-maven-app'
            }
               
        }
        stage('continuous build') {
            steps {
                sh 'mvn clean install'
            }
            }
}
   
}
